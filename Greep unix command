#include<stdio.h>
#include<string.h>

void main(int argc , char *argv[])
{
               FILE *fp;
               char line [100];
              fp = fopen(argv[2],"r");
             while(fscanf(fp , "%[^\n]\n" , line)!=EOF)
            {
                      if(strstr(line , argv[1]) !=NULL)
                     {
                             // print that line
                             printf("%s\n" , line);
                      }
                     else
                    {
                            continue;
                    }
           }
            fclose(fp);
}
